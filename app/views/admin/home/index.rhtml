<% content_for(:secondary) do -%>
	<div class="show-secondary">
				
		<h2>Scorecard</h2>

		<table class="sidetable">
		    <caption class="sidetable">Completed Auctions</caption>
		    <thead >
		      <tr class="sidetable">
		        <th class="sidetable">(to&nbsp;date)</th>
				<th class="sidetable">(last&nbsp;24&nbsp;hrs)</th>
				<th class="sidetable">(last&nbsp;72&nbsp;hrs)</th>
		      </tr>
		    </thead>
		    <tbody>
		      <tr class="sidetable">  
		        <td class="sidetable"><%= @auctions_completed -%></td>
				<td class="sidetable"><%= @auctions_completed_last_72hrs -%></td>
				<td class="sidetable"><%= @auctions_completed_last_24hrs -%></td>
		      </tr>
		    </tbody>
		  </table>
		
		<table class="sidetable">
		    <caption class="sidetable">Active Auctions</caption>
		    <thead >
		      <tr class="sidetable">
		        <th class="sidetable">Auctions</th>
		        <th class="sidetable">Bids</th>
				<th class="sidetable">Expiring&nbsp;w/in&nbsp;24&nbsp;hrs</th>
		      </tr>
		    </thead>
		    <tbody>
		      <tr class="sidetable">  
		        <td class="sidetable"><%= @auctions_count -%></td>
		        <td class="sidetable"><%= @bids_count -%></td>
				<td class="sidetable"><%= @expiring_count -%></td>
		      </tr>
		    </tbody>
		  </table>

		<table class="sidetable">
		    <caption class="sidetable">New Users</caption>
		    <thead >
		      <tr class="sidetable">
		        <th class="sidetable">All</th>
				<th class="sidetable">Nixies</th>
		        <th class="sidetable">Guests</th>
				<th class="sidetable">Members</th>
		      </tr>
		      <tr class="sidetable">
				<th class="sidetable">(to&nbsp;date)</th>
				<th class="sidetable">(no&nbsp;visit)</th>
		        <th class="sidetable">(visited)</th>
				<th class="sidetable">(traders)</th>
		      </tr>
		    </thead>
		    <tbody>
		      <tr class="sidetable">  
	        <td class="sidetable"><%= link_to @users_count, '/users' %></td>
			<td class="sidetable"><%= link_to @nixies_count, '/users?type=nixies' %></td>
	        <td class="sidetable"><%= link_to @guests_count, '/users?type=guests' %></td>
			<td class="sidetable"><%= link_to @members_count, '/users?type=members' %></td>
		      </tr>
		      <tr class="sidetable">
				<th class="sidetable">&nbsp;(last&nbsp;7&nbsp;days)</th>
				<th class="sidetable">&nbsp;</th>
		        <th class="sidetable">&nbsp;</th>
				<th class="sidetable">&nbsp;</th>
		      </tr>
		      <tr class="sidetable">  
				<td class="sidetable"><%= @users_count_last_7_days -%></td>
				<td class="sidetable"><%= @nixies_count_last_7_days -%></td>
		        <td class="sidetable"><%= @guests_count_last_7_days -%></td>
				<td class="sidetable"><%= @members_count_last_7_days -%></td>
		        
		      </tr>
		      <tr class="sidetable">
				<th class="sidetable">&nbsp;(last&nbsp;24&nbsp;hrs)</th>
				<th class="sidetable">&nbsp;</th>
		        <th class="sidetable">&nbsp;</th>
				<th class="sidetable">&nbsp;</th>
		      </tr>
		      <tr class="sidetable"> 
			    <td class="sidetable"><%= @users_count_last_day -%></td>
				<td class="sidetable"><%= @nixies_count_last_day -%></td>
		        <td class="sidetable"><%= @guests_count_last_day -%></td>
				<td class="sidetable"><%= @members_count_last_day -%></td>
		      </tr>
		    </tbody>
		  </table>

		<table class="sidetable">
		    <caption class="sidetable">New Users This Week</caption>
		    <thead >
		      <tr class="sidetable">
				<th class="sidetable">&nbsp;</th>
		        <th class="sidetable">All</th>
				<th class="sidetable">Nixies</th>
		        <th class="sidetable">Guests</th>
				<th class="sidetable">Members</th>
		      </tr>
		    </thead>
		    <tbody>
			  <tr class="sidetable">
				<td class="sidetable"><%= (8.days.ago.beginning_of_day - 7.hours).strftime("%m/%d") -%></td>
		        <td class="sidetable"><%= @all_count_8days_ago -%></td>
				<td class="sidetable"><%= @nixies_count_8days_ago -%></td>
		        <td class="sidetable"><%= @guests_count_8days_ago -%></td>
				<td class="sidetable"><%= @members_count_8days_ago -%></td>
		      </tr>
			  <tr class="sidetable">
				<td class="sidetable"><%= (7.days.ago.beginning_of_day - 7.hours).strftime("%a") -%></td>
		        <td class="sidetable"><%= @all_count_7days_ago -%></td>
				<td class="sidetable"><%= @nixies_count_7days_ago -%></td>
		        <td class="sidetable"><%= @guests_count_7days_ago -%></td>
				<td class="sidetable"><%= @members_count_7days_ago -%></td>
		      </tr>
			  <tr class="sidetable">
				<td class="sidetable"><%= (6.days.ago.beginning_of_day - 7.hours).strftime("%a") -%></td>
		        <td class="sidetable"><%= @all_count_6days_ago -%></td>
				<td class="sidetable"><%= @nixies_count_6days_ago -%></td>
		        <td class="sidetable"><%= @guests_count_6days_ago -%></td>
				<td class="sidetable"><%= @members_count_6days_ago -%></td>
		      </tr>
			  <tr class="sidetable">
				<td class="sidetable"><%= (5.days.ago.beginning_of_day - 7.hours).strftime("%a") -%></td>
		        <td class="sidetable"><%= @all_count_5days_ago -%></td>
				<td class="sidetable"><%= @nixies_count_5days_ago -%></td>
		        <td class="sidetable"><%= @guests_count_5days_ago -%></td>
				<td class="sidetable"><%= @members_count_5days_ago -%></td>
		      </tr>
			  <tr class="sidetable">
				<td class="sidetable"><%= (4.days.ago.beginning_of_day - 7.hours).strftime("%a") -%></td>
		        <td class="sidetable"><%= @all_count_4days_ago -%></td>
				<td class="sidetable"><%= @nixies_count_4days_ago -%></td>
		        <td class="sidetable"><%= @guests_count_4days_ago -%></td>
				<td class="sidetable"><%= @members_count_4days_ago -%></td>
		      </tr>
		      <tr class="sidetable">
				<td class="sidetable"><%= (3.days.ago.beginning_of_day - 7.hours).strftime("%a") -%></td>
		        <td class="sidetable"><%= @all_count_3days_ago -%></td>
				<td class="sidetable"><%= @nixies_count_3days_ago -%></td>
		        <td class="sidetable"><%= @guests_count_3days_ago -%></td>
				<td class="sidetable"><%= @members_count_3days_ago -%></td>
		      </tr>
		      <tr class="sidetable"> 
				<td class="sidetable"><%= (2.days.ago.beginning_of_day - 7.hours).strftime("%a") -%></td>
		        <td class="sidetable"><%= @all_count_2days_ago -%></td>
				<td class="sidetable"><%= @nixies_count_2days_ago -%></td>
		        <td class="sidetable"><%= @guests_count_2days_ago -%></td>
				<td class="sidetable"><%= @members_count_2days_ago -%></td>
		      </tr>
		      <tr class="sidetable"> 
				<td class="sidetable"><%= (1.day.ago.beginning_of_day - 7.hours).strftime("%a") -%></td>
		        <td class="sidetable"><%= @all_count_1days_ago -%></td>
				<td class="sidetable"><%= @nixies_count_1days_ago -%></td>
		        <td class="sidetable"><%= @guests_count_1days_ago -%></td>
				<td class="sidetable"><%= @members_count_1days_ago -%></td>
		      </tr>
		      <tr class="sidetable"> 
				<td class="sidetable">Today</td>
		        <td class="sidetable"><%= @all_count_0days_ago -%></td>
				<td class="sidetable"><%= @nixies_count_0days_ago -%></td>
		        <td class="sidetable"><%= @guests_count_0days_ago -%></td>
				<td class="sidetable"><%= @members_count_0days_ago -%></td>
		      </tr>
		    </tbody>
		  </table>
				
		<table class="sidetable">
		    <caption class="sidetable">Seller/Buyer Activity</caption>
		    <thead >
		      <tr class="sidetable">
		        <th class="sidetable">Members</th>
		        <th class="sidetable">Sellers</th>
				<th class="sidetable">Buyers</th>
		      </tr>
		      <tr class="sidetable">
				<th class="sidetable" colspan="2">(visited&nbsp;to&nbsp;date)</th>
				<th class="sidetable">&nbsp;</th>
		        <th class="sidetable">&nbsp;</th>
				<th class="sidetable">&nbsp;</th>
		      </tr>
		    </thead>
		    <tbody>
		      <tr class="sidetable">  
			<td class="sidetable"><%= link_to @members_count, '/users?type=members' %></td>
		    <td class="sidetable"><%= link_to @sellers_count, '/users?type=sellers' %></td>
			<td class="sidetable"><%= link_to @buyers_count, '/users?type=buyers' %></td>
		      </tr>
		      <tr class="sidetable">
		<th class="sidetable" colspan="2">&nbsp;(visited&nbsp;last&nbsp;72&nbsp;hrs)</th>
				<th class="sidetable">&nbsp;</th>
		        <th class="sidetable">&nbsp;</th>
		      </tr>
		      <tr class="sidetable">  
		        <td class="sidetable"><%= @members_count_logins_last_72_hrs -%></td>
		        <td class="sidetable"><%= @sellers_count_logins_last_72_hrs -%></td>
				<td class="sidetable"><%= @buyers_count_logins_last_72_hrs -%></td>
		      </tr>
		      <tr class="sidetable">
		<th class="sidetable" colspan="2">&nbsp;(visited&nbsp;last&nbsp;24&nbsp;hrs)</th>
				<th class="sidetable">&nbsp;</th>
		        <th class="sidetable">&nbsp;</th>
		      </tr>
		      <tr class="sidetable">  
		        <td class="sidetable"><%= @members_count_logins_last_day -%></td>
		        <td class="sidetable"><%= @sellers_count_logins_last_day -%></td>
				<td class="sidetable"><%= @buyers_count_logins_last_day -%></td>
		      </tr>
			      <tr class="sidetable"><th class="sidetable" colspan="3">&nbsp;(active&nbsp;last&nbsp;24&nbsp;hrs,&nbsp;inc.&nbsp;admin)</th>
					<th class="sidetable">&nbsp;</th>
			        <th class="sidetable">&nbsp;</th>
			      </tr>
			      <tr class="sidetable">  
			        <td class="sidetable"><%= @members_count_logins_last_day -%></td>
			        <td class="sidetable"><%= @sellers_active_last_day -%></td>
					<td class="sidetable"><%= @buyers_active_last_day -%></td>
			      </tr>
		    </tbody>
		  </table>
									
	</div>	
<% end -%>

