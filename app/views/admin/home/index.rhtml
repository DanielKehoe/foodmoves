							<% if user.flag_for_user.nil? -%><%= image_tag "flag_blue.png", :alt=>'Flag', :title=>"no flag set" %><% else -%><%= image_tag "flag_#{user.flag_for_user.color}.png", :alt=>'Flag', :title=>"#{user.flag_for_user.name} user" %><% end -%>&nbsp;<%=h user.last_name -%>&nbsp;<%= local_time user.last_login_at -%>
						<% end -%>
